# AWS Configuration (replace with your actual values in .env.local)
VITE_AWS_REGION=your-aws-region
VITE_AWS_USER_POOL_ID=your-user-pool-id
VITE_AWS_USER_POOL_CLIENT_ID=your-client-id
VITE_AWS_IDENTITY_POOL_ID=your-identity-pool-id

# API Configuration
VITE_API_URL=your-api-gateway-url

# WebSocket Configuration (optional - for real-time features)
VITE_WEBSOCKET_URL=your-websocket-url

# S3 Configuration
VITE_S3_BUCKET=your-s3-bucket-name

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEMO_MODE=false

# Clerk Authentication (get these from your Clerk dashboard)
VITE_CLERK_PUBLISHABLE_KEY=your-clerk-publishable-key
VITE_CLERK_FRONTEND_API=your-clerk-frontend-api
VITE_CLERK_JWKS_URL=your-clerk-jwks-url
